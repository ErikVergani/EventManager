package com.univates.api.exception;


/**
 * @author ev
 */
public class UserAlreadyRegisteredException extends RuntimeException
{
    /**
     * UserAlreadyRegisteredException
     *
     */
    public UserAlreadyRegisteredException()
    {
        super( "Usuário já registrado" );
    }
}
