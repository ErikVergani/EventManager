package com.univates.api.exception;


/**
 * @author ev
 */
public class NotFoundException extends RuntimeException
{
    private final String event;

    /**
     * NotFoundException
     *
     */
    public NotFoundException( String message, String event )
    {
        super( message );
        this.event = event;
    }
    
    
    /**
     * getEvent
     *
     * @return event String
     */
    public String getEvent()
    {
        return event;
    }
}
